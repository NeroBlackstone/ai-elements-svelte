		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.13",
		"tw-animate-css": "^1.3.8",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.0",
		"vite": "^7.1.6",
		"zod": "^4.1.11"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"@ai-sdk/svelte": "^3.0.48",
		"ai": "^5.0.48"
	}
